div#location
	-var locationList = path.split("/");
	ul
		-for(var i=0;i<locationList.length;i++)
			li=locationList[i]
		-var name = aniName.replace(locationList[locationList.length-1]+ " - ", "")
		-var pattern = /.*\s-\s\d\d/;
		-name = name.replace(/\[.*\]/gi, "").replace(".mp4", "");
		-if(pattern.test(name)) name = pattern.exec(name);
		-else name = name.replace(/\(.*\)/gi, "")
		li=name
